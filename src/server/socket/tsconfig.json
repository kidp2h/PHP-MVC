{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["es2015"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true
  }
}
